		"placeholders": {
			"price": {
				"content": "$1",
				"example": "1,99€"
			}
		}
	},
	"app_lowest_price_discount": {
		"message": "SteamDB에 기록된 최저가격: <b> $price$ </b> <b>-$discount$%</b>",
		"placeholders": {
			"price": {
				"content": "$1",
				"example": "1,99€"
			},
			"discount": {
				"content": "$2",
				"example": "80"
			}
		}
	},
	"app_lowest_price_limited": {
		"message": "\"SteamDB\"에 기록된 최저가격은 <b> $price$ </b>이고 최근 2년 이내 최저가격은 <b> $price_limited$ </b>입니다",
		"placeholders": {
			"price": {
				"content": "$1",
				"example": "1,99€"
			},
			"price_limited": {
				"content": "$2",
				"example": "5,99€"
			}
		}
	},
	"app_lowest_date": {
		"message": "$date$에 표시된 가격입니다 ($relative$)",
		"placeholders": {
			"date": {
				"content": "$1",
				"example": "22 Aug 2018"
			},
			"relative": {
				"content": "$2",
				"example": "71 months ago"
			}
		}
	},
	"app_lowest_date_multiple": {
		"message": "$seen$번 표시된 가격이며 마지막으로 $date$에 표시되었습니다 ($relative$)",
		"description": "This string is only used if $seen$ is higher than one, so it can not be 0 or 1.",
		"placeholders": {
			"date": {
				"content": "$1",
				"example": "22 Aug 2018"
			},
			"relative": {
				"content": "$2",
				"example": "71 months ago"
			},
			"seen": {
				"content": "$3",
				"example": "15"
			}
		}
	}
