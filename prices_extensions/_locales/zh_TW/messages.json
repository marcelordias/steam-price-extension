		"placeholders": {
			"price": {
				"content": "$1",
				"example": "1,99€"
			}
		}
	},
	"app_lowest_price_discount": {
		"message": "SteamDB 歷史最低價格為 <b>$price$</b>/<b>-$discount$%</b>",
		"placeholders": {
			"price": {
				"content": "$1",
				"example": "1,99€"
			},
			"discount": {
				"content": "$2",
				"example": "80"
			}
		}
	},
	"app_lowest_price_limited": {
		"message": "SteamDB 歷史最低價格為 <b>$price$</b>，近兩年最低價格為 <b>$price_limited$</b>",
		"placeholders": {
			"price": {
				"content": "$1",
				"example": "1,99€"
			},
			"price_limited": {
				"content": "$2",
				"example": "5,99€"
			}
		}
	},
	"app_lowest_date": {
		"message": "此價格於 $date$ 出現 ($relative$)",
		"placeholders": {
			"date": {
				"content": "$1",
				"example": "22 Aug 2018"
			},
			"relative": {
				"content": "$2",
				"example": "71 months ago"
			}
		}
	},
	"app_lowest_date_multiple": {
		"message": "價格已出現 $seen$ 次，上次出現在 $date$ ($relative$)",
		"description": "This string is only used if $seen$ is higher than one, so it can not be 0 or 1.",
		"placeholders": {
			"date": {
				"content": "$1",
				"example": "22 Aug 2018"
			},
			"relative": {
				"content": "$2",
				"example": "71 months ago"
			},
			"seen": {
				"content": "$3",
				"example": "15"
			}
		}
	}
